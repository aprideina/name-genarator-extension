{
    "manifest_version": 2,
    "name": "Name Generation",
    "description": "This extension will generate random person information",
    "version": "1.0",
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "https://randomuser.me/api/",
        "http://api.namefake.com/",
        "storage"
    ]
}