{
    "manifest_version": 2,
    "name": "Name Generator",
    "description": "This extension will generate random person information using different external sources",
    "version": "0.0.0.1",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": { "16": "img/icon16.png",
               "48": "img/icon48.png",
               "64": "img/icon64.png",
              "128": "img/icon128.png" 
    },
    "permissions": [
        "https://randomuser.me/api/",
        "http://api.namefake.com/",
        "storage"
    ],
    "incognito": "split",
    "short_name": "Name Generator"
}